cutoff x = ccv x # ccn 2
reson x = ccv x # ccn 3
sweept x = ccv x # ccn 4
envmod x = ccv x # ccn 5
envatt x = ccv x # ccn 6
envdec x = ccv x # ccn 7
accdec x = ccv x # ccn 8
envacc x = ccv x # ccn 9
acidcof =
    p "acidcof" $
    (12.6 ~>) $ 
    cutoff (
        ( slow 64 $ range 8 119 sine )
        + ( slow 8 $ range "-8" 8 tri )
        ) # "acid"
acidres =
    p "acidres" $
    (64 ~>) $ 
    reson ( slow 76 $ ( 127 - range 0 127 tri) ) # "acid"
acidenm =
    p "acidenm" $
    (32 ~>) $
    envmod (
        ( slow 64 ( range 64 95 tri) )
        + ( slow 132 $ range "-64" 32 sine )
    ) # "acid"
acidatt =
    p "acidatt" $
    (12.6 ~>) $ 
    envatt (
        ( slow 48 $ range 16 32 saw )
        + ( slow 4 $ range "-4" 4 tri )
        ) # "acid"
aciddec =
    p "acidedec" $
    (12.6 ~>) $ 
    envdec (
        ( slow 96 $ range 11 123 saw )
        + ( slow 6 $ range "-4" 4 tri )
        ) # "acid"
acidacd =
    p "acidacd" $
    (36 ~>) $ 
    accdec (
        ( slow 112 $ range 11 123 tri )
        + ( slow 4 $ range "-4" 4 tri )
        ) # "acid"
acideac =
    p "acideacc" $
    (19.84 ~>) $ 
    envacc (
        ( slow 84 $ range 0 97 sine )
        * ( slow 12 $ tri )
        + 30
        ) # "acid"
acidmod =
    do
    acidcof
    acidres
    acidenm
    acidatt
    acidacd
    acideac
    aciddec
acidmstop =
    do
    p "acidcof" silence
    p "acidres" silence
    p "acidenm" silence
    p "acidatt" silence
    p "acidacd" silence
    p "acideac" silence
    p "aciddec" silence
